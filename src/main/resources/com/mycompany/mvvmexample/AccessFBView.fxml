<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="506.0" prefWidth="496.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="modelview.AccessFBView">
   <children>
      <VBox alignment="CENTER_RIGHT" layoutX="7.0" prefHeight="190.0" prefWidth="485.0" spacing="10.0">
         <children>
            <TextField fx:id="nameField" alignment="TOP_LEFT" promptText="Name" />
            <TextField fx:id="majorField" promptText="Major" />
            <TextField fx:id="ageField" promptText="Age" />
            <Button fx:id="writeButton" mnemonicParsing="false" onAction="#addRecord" text="Write" textAlignment="CENTER" />
            <Button fx:id="readButton" mnemonicParsing="false" onAction="#readRecord" text="Read" textAlignment="CENTER" />
            <Button fx:id="updateButton" mnemonicParsing="false" onAction="#updateData" text="Update" textAlignment="CENTER" />
            <Button fx:id="removeButton" mnemonicParsing="false" onAction="#removeData" text="Remove" textAlignment="CENTER" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <TableView fx:id="tableField" layoutX="14.0" layoutY="266.0" onMouseClicked="#rowSelected" prefHeight="237.0" prefWidth="465.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="157.0" text="Name" />
            <TableColumn fx:id="majorColumn" prefWidth="157.0" text="Major" />
            <TableColumn fx:id="ageColumn" prefWidth="150.0" text="Age" />
        </columns>
      </TableView>
      <HBox layoutX="341.0" layoutY="462.0" prefHeight="46.0" prefWidth="155.0" spacing="10.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
